@model Hirafeyat.ViewModel.ChangeProfileViewModel
<h2>Change Personal Info</h2>
<form asp-action="ChangePersonalInfo" method="post">
    <input asp-for="FullName" class="form-control mb-2" />
    <input asp-for="Email" class="form-control mb-2" />
    <input asp-for="Address" class="form-control mb-2" />
    <input asp-for="PhoneNumber" class="form-control mb-2" />
    <button type="submit" class="btn btn-primary">Save</button>
</form>


